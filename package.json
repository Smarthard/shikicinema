{
  "name": "shikicinema",
  "version": "0.8.14",
  "description": "Returns video player on Shikimori",
  "main": "src/shikicinema.js",
  "scripts": {
    "install-deps": "npm i && cd src/ui && npm i && cd ../../",
    "bundle": "cd src/ui && ng b && cd ../.. && webpack --mode development",
    "release": "cd src/ui && ng b --prod && cd ../.. && NODE_ENV=production webpack --mode production",
    "watch": "webpack --mode development && cd src/ui && ng b --watch --delete-output-path=false",
    "test": "echo 'no tests specified'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Smarthard/shikicinema.git"
  },
  "keywords": [
    "extension",
    "shikimori"
  ],
  "author": "Smarthard",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/Smarthard/shikicinema/issues"
  },
  "homepage": "https://github.com/Smarthard/shikicinema",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.1.1",
    "eslint": "^7.9.0",
    "eslint-loader": "^4.0.2",
    "terser-webpack-plugin": "^4.2.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
