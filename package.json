{
    "name": "shikicinema",
    "version": "1.0.0",
    "description": "Returns video player on Shikimori",
    "main": "src/shikicinema.js",
    "engines": {
        "node": ">=16"
    },
    "private": true,
    "keywords": [
        "extension",
        "shikimori"
    ],
    "author": "Smarthard",
    "license": "BSD-2-Clause",
    "bugs": {
        "url": "https://github.com/Smarthard/shikicinema/issues"
    },
    "homepage": "https://github.com/Smarthard/shikicinema",
    "repository": {
        "type": "git",
        "url": "https://github.com/Smarthard/shikicinema.git"
    },
    "scripts": {
        "ng": "ng",
        "cap": "cap",
        "esrun": "esrun",
        "npx": "npx",
        "ionic": "ionic",
        "webpack-bundle-analyzer": "webpack-bundle-analyzer",
        "start": "ionic serve",
        "start:native": "npm-run-all -s 'ionic cap sync' 'npx cap sync @capacitor-community/electron' 'npx cap open @capacitor-community/electron'",
        "build": "ionic build",
        "build:prod": "ionic build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "set-env": "esrun setup-env.ts",
        "watch": "ionic build --watch --configuration development",
        "analyze-bundle": "npm-run-all -s 'ionic build -b -- --stats-json' 'webpack-bundle-analyzer dist/stats.json'"
    },
    "workspaces": [
        "electron"
    ],
    "pre-commit": [
        "lint"
    ],
    "dependencies": {
        "@angular/animations": "~16.2.12",
