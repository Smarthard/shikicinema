        "@angular/language-service": "~16.2.12",
        "@capacitor/cli": "^5.5.1",
        "@digitak/esrun": "^3.2.25",
        "@ionic/angular-toolkit": "^10.0.0",
        "@ionic/cli": "^7.1.1",
        "@ngrx/eslint-plugin": "^16.3.0",
        "@ngrx/schematics": "^16.3.0",
        "@ngrx/store-devtools": "16.3.0",
        "@types/chrome": "0.0.249",
        "@types/jasmine": "~5.1.1",
